	</parent>
	<groupId>com.course.first</groupId>
	<artifactId>FirstSpringProject</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>FirstSpringProject</name>
	<description>First project in Spring Boot</description>

	<properties>
		<java.version>1.8</java.version>
		<start-class>net.javaguides.springboot.OBAASApplication</start-class>
